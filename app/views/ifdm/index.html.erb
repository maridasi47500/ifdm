<%=render Webbillet.recent.limit(1)%>
<a class="inscrire individuel" href="<%=coursindividuel_path%>" onmouseout="this.classList.remove('myarrow');this.classList.add('myarrowback');setTimeout('function1(\'individuel\')', 1000);" onmouseover="this.classList.add('myarrow');setTimeout('function2(\'individuel\')', 1000);">Cours individuels<span> > </span></a>
<article>
<%=link_to coursindividuel_path do%>
<img class="mycourse" src="/assets/COURS-INDIVIDUELS" />
<%end%>
</article>

<a class="inscrire collectif" href="<%=courscollectif_path%>" onmouseout="this.classList.remove('myarrow');this.classList.add('myarrowback');setTimeout('function1(\'collectif\')', 1000);" onmouseover="this.classList.add('myarrow');setTimeout('function2(\'collectif\')', 1000);">Cours collectifs<span> > </span></a>
<article>
<%=link_to courscollectif_path do%>
<img class="mycourse" src="/assets/COURS-COLLECTIFS" />
<%end%>
</article>
<a class="inscrire stage" href="/nos-stages" onmouseout="this.classList.remove('myarrow');this.classList.add('myarrowback');setTimeout('function1(\'stage\')', 1000);" onmouseover="this.classList.add('myarrow');setTimeout('function2(\'stage\')', 1000);">Stages<span> > </span></a>
<article>
<%=link_to "/cours-collectifs" do%>
<img class="mycourse" src="/assets/COURS-COLLECTIFS" />
<%end%>
</article>
